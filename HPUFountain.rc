IDR_MAINFRAME MENU PRELOAD DISCARDABLE 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建(&N)\tCtrl+N",            ID_FILE_NEW
        MENUITEM "打开(&O)...\tCtrl+O",         ID_FILE_OPEN
        MENUITEM "保存(&S)\tCtrl+S",            ID_FILE_SAVE
        MENUITEM "另存为(&A)...",               ID_FILE_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "打印(&P)...\tCtrl+P",         ID_FILE_PRINT
        MENUITEM "打印预览(&V)",                ID_FILE_PRINT_PREVIEW
        MENUITEM "打印设置(&R)...",             ID_FILE_PRINT_SETUP
        MENUITEM SEPARATOR
        MENUITEM "最近文件",                    ID_FILE_MRU_FILE1, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                    ID_APP_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "撤消(&U)\tCtrl+Z",            ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "剪切(&T)\tCtrl+X",            ID_EDIT_CUT
        MENUITEM "复制(&C)\tCtrl+C",            ID_EDIT_COPY
        MENUITEM "粘贴(&P)\tCtrl+V",            ID_EDIT_PASTE
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "工具栏(&T)",                  ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                  ID_VIEW_STATUS_BAR
    END
    MENUITEM "恢复",                        IDM_HUIFU
    POPUP "图形变换造型"
    BEGIN
        MENUITEM SEPARATOR
        MENUITEM "旋转变换",                    IDM_MODEL40
        MENUITEM "平移变换",                    IDM_MODEL41
        MENUITEM "对称变换",                    IDM_MODEL42
        MENUITEM SEPARATOR
    END
    POPUP "特殊造型"
    BEGIN
        MENUITEM "回转型",                      IDM_MODEL1
        MENUITEM "HPU造型",                     IDM_MODEL9
        MENUITEM "茶壶瀑布",                    IDM_MODEL4
        MENUITEM "圆台",                        IDM_MODEL5
        MENUITEM "五角星",                      IDM_MODEL15
        MENUITEM SEPARATOR
        MENUITEM "四叶花",                      IDM_MODEL10
        MENUITEM "牵牛花",                      IDM_MODEL12
        MENUITEM "塔造型",                      IDM_MODEL33
        MENUITEM "皇冠",                        IDM_MODEL2
    END
